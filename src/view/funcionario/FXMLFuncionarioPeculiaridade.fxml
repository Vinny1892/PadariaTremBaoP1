<AnchorPane id="AnchorPane" prefHeight="444.0" prefWidth="775.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.funcionario.FXMLFuncionarioPeculiaridade">
    <children>
        <Label layoutX="22.0" layoutY="27.0" prefHeight="24.0" prefWidth="213.0" text="Produtos faltantes no estoque">

            </children>
        </AnchorPane>
        <Button fx:id="btnPadeiroConf" layoutX="275.0" layoutY="248.0" mnemonicParsing="false" onAction="#btnPadeiroConfAction" prefHeight="49.0" prefWidth="133.0" text="Configuração Padeiro" />
